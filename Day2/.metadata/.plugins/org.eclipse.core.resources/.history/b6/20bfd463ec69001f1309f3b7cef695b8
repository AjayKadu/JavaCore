
public class GetSetMethod {

	public static void main(String[] args) {
		
		Person p = new Person();
		p.getAge();

	}

}

class Person{
	
	private String name;
	private int age;
	
	
	public String getName() {
		return name;
	}
	
	public void setName() {
		name = name;
	}
	
	public int getAge() {
		return age;
	}
	
	public void setAge() {
		age = age;
	}
}
